{
  "name": "graphql-extended",
  "version": "0.0.48",
  "description": "An extension of GraphQL adding higher order functionality.",
  "author": "Michael Paris <michael@scaphold.io>",
  "license": "MIT",
  "homepage": "https://scaphold.io",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scaphold-io/graphql-extended"
  },
  "bugs": {
    "url": "https://github.com/scaphold-io/graphql-extended/issues"
  },
  "keywords": [
    "graphql",
    "javascript",
    "typescript",
    "emca",
    "emca2016",
    "graphql",
    "npm",
    "microservices"
  ],
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build"
  ],
  "bin": {},
  "scripts": {
    "buildw": "./scripts/buildw",
    "build": "./scripts/build",
    "dev": "./scripts/dev",
    "lint": "./scripts/lint",
    "ci": "./scripts/ci",
    "test": "jest",
    "coverage": "jest --coverage",
    "check": "npm run lint && npm run test",
    "patch-release": "npm version patch && npm publish && git push --follow-tags",
    "build-dot-flow": "./scripts/build-types"
  },
  "dependencies": {
    "graphql": "^0.10.1",
    "immutable": "^3.8.1",
    "iterall": "^1.1.1",
    "tslib": "^1.5.0"
  },
  "devDependencies": {
    "@types/graphql": "^0.9.0",
    "@types/immutable": "^3.8.7",
    "@types/jest": "^19.2.2",
    "@types/node": "^7.0.29",
    "graphql": "^0.10.1",
    "install": "^0.10.1",
    "jest": "^20.0.4",
    "nodemon": "^1.11.0",
    "npm": "^4.4.1",
    "source-map-support": "^0.4.6",
    "supertest": "^3.0.0",
    "ts-jest": "^20.0.6",
    "ts-node": "^3.0.6",
    "tslint": "^5.4.3",
    "typescript": "^2.2.1"
  },
  "jest": {
    "transform": {
      ".*": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "browser": false,
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$"
  }
}
